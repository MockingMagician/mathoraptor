{
    "name": "mocking-magician/mathoraptor",
    "type": "library",
    "description": "Dealing with big numbers in PHP",
    "license": "CC-BY-SA-4.0",
    "homepage": "https://github.com/MockingMagician/mathoraptor",
    "keywords": [
        "math", "big number"
    ],
    "authors": [
        {
            "name": "Marc MOREAU",
            "email": "moreau.marc.web@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "MockingMagician\\Mathoraptor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MockingMagician\\Mathoraptor\\Benchmarks\\": "benchmarks",
            "MockingMagician\\Mathoraptor\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.1.0",
        "ext-bcmath": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "@dev",
        "phpbench/phpbench": "@dev"
    }
}
